{% extends 'base.html.twig' %}

{% block title %}Créer un Set de Santons{% endblock %}

{% block body %}
<div class="contact-form-container">
    <h1>Créer un nouveau Set de Santons</h1>

    {{ form_start(form, {'attr': {'class': 'form'}}) }}
        <div class="form-group">
        {{ form_widget(form) }}
        </div>
        <button type="submit" class="voir-plus">Créer</button>
    {{ form_end(form) }}
</div>
{% endblock %}